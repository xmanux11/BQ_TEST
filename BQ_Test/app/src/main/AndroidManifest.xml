<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.manumadrid.bqtest">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.manumadrid.bqtest.MainActivity" android:label="@string/app_name"/>
        <activity android:name="com.example.manumadrid.bqtest.CreateNote" android:label="@string/app_name"/>
        <activity android:name="com.example.manumadrid.bqtest.NoteFragment" android:label="@string/app_name"/>
        <activity android:name="com.evernote.client.android.EvernoteOAuthActivity"/>
        <activity android:name="com.evernote.client.android.login.EvernoteLoginActivity"/>
    </application>





</manifest>